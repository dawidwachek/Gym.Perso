{
	"builds":[{
		"src":"trainingstudio/wsgi.py",
		"use":"@vercel/python@3.0.7",
		"config":{"maxLambdaSize": "15mb", "runtime":"python3.9"}
	}],
	"routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        }
		{
			"src":"/(.*)",
			"dest":"trainingstudio/wsgi.py"
		}
	]


}
